imap -ex <C-n> bn
imap -ex <C-p> bp

imap <A-b> <C-left>
imap <A-f> <C-right>

nmap -ex -count > tabm +1
nmap -ex -count <lt> tabm -1

nmap -builtin -count j 5j
nmap -builtin -count k 5k

colorscheme base16

nmap <cr> <nop>
nmap g<esc> <nop>
nmap z<esc> <nop>
map -modes menu <esc> <nop>

set defsearch=duckduckgo
